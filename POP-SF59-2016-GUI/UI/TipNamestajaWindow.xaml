<Window x:Class="POP_SF59_2016_GUI.UI.TipNamestajaWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF59_2016_GUI.UI"
        mc:Ignorable="d"
        Title="TipNamestajaWindow" Height="109" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="81*"/>
            <ColumnDefinition Width="211*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="19*"/>
            <RowDefinition Height="21*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Content="Naziv"/>
        <TextBox Grid.Row="0" Grid.Column="1" Name="tbNaziv" HorizontalContentAlignment="Center"/>
        <Button Grid.Row="1" Content="Sacuvaj" Height="40" Width="80" Grid.Column="1" Margin="10,0,121,0.95" Click="SacuvajIzmene"/>
        <Button Grid.Row="1" Content="Izadji" Height="40" Width="80" Grid.Column="1" Margin="121,0,10,0.95" Click="Izadji"/>
    </Grid>
</Window>
